<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!--mysql 连接数据库jar 这里选择自己本地位置-->

    <classPathEntry location="C:/Users/xiezy/.m2/repository/mysql/mysql-connector-java/8.0.11/mysql-connector-java-8.0.11.jar" />

    <context id="testTables" targetRuntime="MyBatis3">
        <!-- 自动识别数据库关键字，默认false，如果设置为true，根据SqlReservedWords中定义的关键字列表；
            一般保留默认值，遇到数据库关键字（Java关键字），使用columnOverride覆盖 -->
        <property name="autoDelimitKeywords" value="false"/>
        <!-- 生成的Java文件的编码-->
        <property name="javaFileEncoding" value="UTF-8"/>
        <!-- 格式化java代码-->
        <property name="javaFormatter" value="org.mybatis.generator.api.dom.DefaultJavaFormatter"/>
        <!-- 格式化XML代码-->
        <property name="xmlFormatter" value="org.mybatis.generator.api.dom.DefaultXmlFormatter"/>

        <!--Example替换成Criteria-->
        <plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
            <property name="searchString" value="Example$"/>
            <property name="replaceString" value="Criteria"/>
        </plugin>

        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"/>

        <!--虚拟组件，针对表没有设置主键的时候使用-->
        <plugin type="org.mybatis.generator.plugins.VirtualPrimaryKeyPlugin" />

        <!--<commentGenerator>-->
        <!--&lt;!&ndash; 是否去除自动生成的注释 true：是 ： false:否 &ndash;&gt;-->
        <!--<property name="suppressAllComments" value="true" />-->
        <!--</commentGenerator>-->

        <!--注释-->
        <commentGenerator>
            <!--<property name="javaFileEncoding" value="UTF-8"/>-->
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>

        <!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://rm-bp1755ydatm21f766eo.mysql.rds.aliyuncs.com:3306/bqhealth_cloud_db?characterEncoding=utf8"
                        userId="bqhealthcloud"
                        password="bqcloud@bq0330">
            <!--设置为 true 可以获取 tables 信息, 解决生成文件缺少 xxxByPrimaryKey 的问题 -->
            <property name="useInformationSchema" value="true"/>
        </jdbcConnection>

        <!-- 默认false，把JDBC DECIMAL 和 NUMERIC 类型解析为 Integer，为 true时把JDBC DECIMAL 和
           NUMERIC 类型解析为java.math.BigDecimal -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!-- targetProject:生成PO类的位置 -->
        <javaModelGenerator targetPackage="com.alijk.bqhealth.cloud.dal.pojo.kpi" targetProject="src/main/java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
            <!-- 从数据库返回的值被清理前后的空格 -->
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!-- mapper映射文件生成的位置-->
        <sqlMapGenerator targetPackage="sqlmapper.kpi" targetProject="src/main/resources">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>

        <!-- targetPackage：mapper接口生成的位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.alijk.bqhealth.cloud.dal.mapper.kpi" targetProject="src/main/java" >
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>

        <!-- 指定数据库表 -->
        <table tableName="hospital_street_info" domainObjectName="HospitalStreetInfo" >
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
            <columnOverride column="is_deleted" javaType="Integer"/>
        </table>

        <table tableName="hospital_indicator_day_result" domainObjectName="HospitalIndicatorDayResult" >
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
            <columnOverride column="is_deleted" javaType="Integer"/>
        </table>

        <!--<table tableName="hospital_street_info" domainObjectName="HospitalStreetInfo" >
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
            <columnOverride column="is_deleted" javaType="Integer"/>
        </table>-->

        <!--<table tableName="hospital_indicator_info" domainObjectName="HospitalIndicatorInfo" >
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
            <columnOverride column="is_deleted" javaType="Integer"/>
        </table>

        <table tableName="hospital_indicator_month_result" domainObjectName="HospitalIndicatorMonthResult" >
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
            <columnOverride column="is_deleted" javaType="Integer"/>
        </table>

        <table tableName="hospital_indicator_year_result" domainObjectName="HospitalIndicatorYearResult" >
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
            <columnOverride column="is_deleted" javaType="Integer"/>
        </table>

        <table tableName="hospital_role_indicator_relation" domainObjectName="HospitalRoleIndicatorRelation" >
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
            <columnOverride column="is_deleted" javaType="Integer"/>
        </table>-->

        <!--## mybatis 代码自动生成
        配置 mbg-mysql.xml 之后；执行 `mvn mybatis-generator:generate`-->
    </context>
</generatorConfiguration>